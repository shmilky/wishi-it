{
  "name": "Wishi Me",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Add items you really own to your Wishi closet",
  "icons": { "16": "wishi16.png",
    "48": "wishi48.png",
    "128": "wishi128.png" },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "exclude_matches": ["http://*.wishi.me/*", "https://*.wishi.me/*", "http://wishi.me/*", "https://wishi.me/*"],
      "css": ["pageContent.css"],
      "js": ["jquery-2.2.0.min.js", "pageContent.js"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": "wishi48.png",
    "default_popup": "popup.html",
    "default_title": "Wishi Me"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://*.wishi.me/"
  ]
}