{
  "name": "Wishi Me",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Put a pin in your favourite images to add to your wishi closet and inspiration board",
  "icons": { "16": "wishi16.png",
    "48": "wishi48.png",
    "128": "wishi128.png" },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["pageContent.css"],
      "js": ["jquery-1.5.1.min.js", "pageContent.js"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": "wishi48.png",
    "default_popup": "popup.html",
    "default_title": "Wishi Me"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://*.wishi.me/"
  ]
}